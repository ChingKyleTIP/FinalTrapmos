{
  "expo": {
    "name": "Trapmos",
    "slug": "your-app-slug",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.trapmos",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7V-8rb-g_OmFfjMOj6wvI1rKGi4QmDOc"
        }
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./images/1.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "d5ffb1b3-e8ab-4ec3-88ea-2276d2b4d3f7"
      }
    }
  }
}
